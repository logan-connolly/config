*background: #282828
*foreground: #c5c8c6
*cursorColor: #c5c8c6

*color0: #282828
*color8: #928374
*color1: #cc241d
*color9: #fb4934
*color2: #98971a
*color10: #b8bb26
*color3: #d79921
