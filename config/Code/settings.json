{
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "Gruvbox Material Dark",
    "python.showStartPage": false,
    "terminal.integrated.automationShell.linux": "/usr/bin/zsh",
    "liveSassCompile.settings.formats": [

        {
            "format": "expanded",
            "extensionName": ".css",
            "savePath": "/dist/css"
        }
    ],
    "gruvboxMaterial.darkWorkbench": "flat",
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.formatOnSave": true,
    "[scss]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.tabSize": 2,
    "explorer.confirmDelete": false,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "terminal.integrated.fontFamily": "'SauceCodePro Nerd Font Mono'",
    "editor.fontFamily": "'SauceCodePro Nerd Font Mono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "window.zoomLevel": 0,
    "python.formatting.provider": "black",
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "liveServer.settings.donotShowInfoMsg": true,
    "workbench.startupEditor": "newUntitledFile",
    "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
    "window.menuBarVisibility": "toggle",
    "window.customMenuBarAltFocus": false,
    "window.enableMenuBarMnemonics": false,
    "workbench.activityBar.visible": true,
    "editor.minimap.enabled": false,
    "gitlens.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.codeLens.enabled": false,
    "workbench.statusBar.visible": true
}